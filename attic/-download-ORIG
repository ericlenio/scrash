-download() {
  local f="$1" ext gzip=1 md5 basename
  if [ ! -e "$f" ]; then
    echo "$FUNCNAME: no such file, or file is not readable: $f"
    return 1
  fi
  ext="${f##*.}"
  md5=$(-md5 "$f")
  basename=$(basename "$f")
  -ws /scr-upload-file --data-binary @"$f" -H "x-file-name: $basename" -H "x-file-md5: $md5"
}

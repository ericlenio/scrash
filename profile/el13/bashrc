export SCR_PS1='\[\e[0;100m\]\u\[\e[0;100m\]@\[\e[0;100m\]\h\[\e[0m\]:\[\e[0m\]\w\[\e[0m\]:\[\e[0m\]\v\[\e[0m\]\$ \[\e[0m\]'
export PS1=$SCR_PS1
export HISTCONTROL=ignoreboth:erasedups
export PAGER=less
export LESS="-siR"
if ! [[ "$PROMPT_COMMAND" =~ -prompt-command$ ]]; then
  # remote shells may set PROMPT_COMMAND, allow for that but additionally still
  # ensure that -prompt-command gets executed too
  export PROMPT_COMMAND="${PROMPT_COMMAND:-:};-prompt-command"
fi
-add-path /usr/local/bin /usr/local/sbin $HOME/bin
export -f v l c
#export SCR_LOCALHOST_FUNCS="-totp -ra -pwgen -linux-lwsecrets -darwin-macbook-pf-init"
export SCR_SSH_CURRENT_KEY="ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIMml8TCd3z4xOWrq0euNHvfLebEeMHiKsHA7Ul1SZ5E2 Eric Lenio 20231106"
export SCR_SSH_OLD_KEY="ssh-rsa  lenio@elmbp.local"
if [ "$SSH_CLIENT" -a ${SCR_SSH_KEY_SWAP_CHECKED:=0} != 1 -a -t 0 -a -t 1 ]; then
  -ssh-swap-key
  export SCR_SSH_KEY_SWAP_CHECKED=1
fi
export PGTZ=America/New_York

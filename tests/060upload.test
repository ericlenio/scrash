# vim:filetype=bash
-clear-cache
[ "$SCR_SSH_USER" ]
[ "$SCR_SSH_USER_KNOWN_HOSTS" ]
[ ! -f "$SCR_SSH_USER_KNOWN_HOSTS" ]
cd $SCR_TMPDIR
-upload /etc/passwd
[ $(-md5 /etc/passwd) = $(-md5 passwd) ]
rm passwd
! -upload /no-such-file-exists
# test upload a file with a space in the name
f="/tmp/passwd file"
cp /etc/passwd "$f"
-upload "$f"
basename=$(basename "$f")
[ $(-md5 /etc/passwd) = $(-md5 "$basename") ]
rm "$f"
cd "$OLDPWD"
[ -s "$SCR_SSH_USER_KNOWN_HOSTS" ]

# vim:filetype=bash
-clear-cache
[ "$SCR_SSH_USER" ]
[ "$SCR_SSH_USER_KNOWN_HOSTS" ]
[ ! -f "$SCR_SSH_USER_KNOWN_HOSTS" ]
cd $SCR_TMPDIR
-upload /etc/services
[ $(-md5 /etc/services) = $(-md5 services) ]
rm services
! -upload /no-such-file-exists
# test upload a file with a space in the name
f="/tmp/services file"
cp /etc/services "$f"
-upload "$f"
basename=$(basename "$f")
[ $(-md5 /etc/services) = $(-md5 "$basename") ]
cd "$OLDPWD"
rm "$f"
[ -s "$SCR_SSH_USER_KNOWN_HOSTS" ]

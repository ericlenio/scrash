# vim:filetype=bash
if false; then
  cd $SCR_TMPDIR
  -upload /etc/passwd
  [ $(-md5 /etc/passwd) = $(-md5 passwd) ]
  rm passwd
  ! -upload /no-such-file-exists
  # test upload a file with a space in the name
  f="/tmp/passwd file"
  cp /etc/passwd "$f"
  -upload "$f"
  basename=$(basename "$f")
  [ $(-md5 /etc/passwd) = $(-md5 "$basename") ]
  rm "$f"
  cd $OLDPWD
fi
